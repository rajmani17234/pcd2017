#include<stdio.h>
struct complex
{
int real;
int img;
};
int main()
{
struct complex a,b,c;
printf("Enter the first complex number:a+ib\n");
printf("a=");
scanf("%d",&a.real);
printf("b=");
scanf("%d",&a.img);
printf("Enter the second complex number:c+id\n");
printf("c=");
scanf("%d",&b.real);
printf("d=");
scanf("%d",&b.img);
c.real=a.real+b.real;
c.img=a.img+b.img;
printf("Sum of two complex number is=%d+%di\n",c.real,c.img)
return 0;
}
